window.wl = window.wl || {};
window.wl.collections = {};

wl.collections.Sensors = Backbone.Collection.extend({
	model: wl.models.SensorModel
});

wl.collections.PortletTypes = Backbone.Collection.extend({
	model: wl.models.PortletType
});

wl.collections.PortletConfigs = Backbone.Collection.extend({
	model: wl.models.PortletConfig,
	comparator: 'iPosition'
});
